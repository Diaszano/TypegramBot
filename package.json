{
  "bugs": {
    "url": "https://github.com/Diaszano/TypegramBot/issues"
  },
  "main": "dist/index.js",
  "name": "typegrambot",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Lucas Dias",
    "url": "https://github.com/Diaszano"
  },
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint ./src",
    "test": "vitest",
    "build": "tsc",
    "prepare": "husky install",
    "lint:fix": "eslint ./src --fix",
    "lint-staged": "lint-staged",
    "dev:start": "clear && ts-node ./src/index.ts",
    "dev:watch": "clear && ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts"
  },
  "homepage": "https://github.com/Diaszano/TypegramBot#readme",
  "keywords": [
    "bot",
    "telegram"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Diaszano/TypegramBot.git"
  },
  "description": "Bot do Telegram desenvolvido em TypeScript com uma ampla variedade de funcionalidades.",
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "colors": "^1.4.0",
    "dotenv": "^16.0.3",
    "dzn-cache": "^0.1.3",
    "linketrackjs": "^1.2.0",
    "telegraf": "^4.12.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prisma": "^4.14.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "ts-prune": "^0.10.3",
    "typescript": "^5.0.4",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.1",
    "vitest-mock-extended": "^1.1.3"
  }
}
